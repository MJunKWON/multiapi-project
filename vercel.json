{
  "version": 2,
  "buildCommand": "npm run build",
  "outputDirectory": "packages/frontend/dist",
  "installCommand": "npm install",
  "framework": null,
  "rewrites": [
    {
      "source": "/(.*)",
      "destination": "/index.html"
    }
  ],
  "regions": ["icn1"],
  "builds": [
    {
      "src": "packages/frontend/package.json",
      "use": "@vercel/static-build"
    }
  ]
}