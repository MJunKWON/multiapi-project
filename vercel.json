{
  "framework": "nextjs",
  "buildCommand": "cd packages/frontend && npm install && npm run build",
  "outputDirectory": "packages/frontend/.next",
  "installCommand": "cd packages/frontend && npm install",
  "rewrites": [
    {
      "source": "/(.*)",
      "destination": "/"
    }
  ],
  "builds": [
    {
      "src": "packages/frontend/package.json",
      "use": "@vercel/next"
    }
  ]
}